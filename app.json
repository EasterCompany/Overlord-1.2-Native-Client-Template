{
  "expo": {
    "name": "app-name",
    "slug": "app-name",
    "version": "0.0.0",
    "platforms": ["ios", "android", "web"],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "app-name",
    "updates": {
      "enabled": false,
      "fallbackToCacheTimeout": 999
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "notification": {
      "androidMode": "collapse",
      "androidCollapsedTitle": "app-name"
    },
    "ios": {
      "bundleIdentifier": "com.eastercompany.app-name",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "requireFullScreen": false
    },
    "android": {
      "package": "com.eastercompany.app-name",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
